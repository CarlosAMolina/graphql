meta {
  name: aggregation_users_int
  type: graphql
  seq: 3
}

post {
  url: {{protocol}}://{{domain}}:{{port}}/{{path}}
  body: graphql
  auth: none
}

body:graphql {
  {
    aggregationUsers(filters: {country: "Spain"}, aggregationAndField:["max", "age"]) {
      integer
    }
  }
}

assert {
  res.status: eq 200
  res.body.data.aggregationUsers.integer: eq 20
}
