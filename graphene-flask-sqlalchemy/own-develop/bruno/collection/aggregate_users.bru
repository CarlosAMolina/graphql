meta {
  name: aggregate_users
  type: graphql
  seq: 1
}

post {
  url: {{protocol}}://{{domain}}:{{port}}/{{path}}
  body: graphql
  auth: none
}

body:graphql {
  query {
    aggregateUsers {
      totalCount
    }
  }
}

assert {
  res.status: eq 200
  res.body.data.aggregateUsers.totalCount: eq 4
}
