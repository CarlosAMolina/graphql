meta {
  name: aggregation_users
  type: graphql
  seq: 2
}

post {
  url: {{protocol}}://{{domain}}:{{port}}/{{path}}
  body: graphql
  auth: none
}

body:graphql {
  {
    aggregationUsers(aggregationAndField:["max", "age"]) {
      integer,
      totalCount
    }
  }
}

assert {
  res.status: eq 200
  res.body.data.aggregateUsersInt: eq 4
}
