meta {
  name: aggregation_users
  type: graphql
  seq: 2
}

post {
  url: {{protocol}}://{{domain}}:{{port}}/{{path}}
  body: graphql
  auth: none
}

body:graphql {
  {
    aggregationUsers(aggregationAndField:["max", "age"]) {
      integer
    }
  }
}

assert {
  res.status: eq 200
  res.body.data.aggregateUsers.integer: eq 41
}
