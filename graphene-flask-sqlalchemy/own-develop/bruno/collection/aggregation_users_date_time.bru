meta {
  name: aggregation_users_date_time
  type: graphql
  seq: 2
}

post {
  url: {{protocol}}://{{domain}}:{{port}}/{{path}}
  body: graphql
  auth: none
}

body:graphql {
  {
    aggregationUsers(filters: {country: "Spain"}, aggregationAndField:["min", "creation_date_time"]) {
      dateTime
    }
  }
}

assert {
  res.status: eq 200
  res.body.data.aggregationUsers.dateTime: eq 2024-03-20T17:20:40
}
